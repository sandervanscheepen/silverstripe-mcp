{
    "name": "silverstripe-mcp/analyzer",
    "description": "PHP code analyzer for Silverstripe 6 compatibility",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SilverstripeMCP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilverstripeMCP\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "nikic/php-parser": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "bin": [
        "bin/analyze"
    ]
}
